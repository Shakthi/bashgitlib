#!/bin/bash
#name:quotes

##version
quotes_imported=1 
#source http://code.google.com/p/yahoo-finance-managed/wiki/csvQuotesDownload
quotes_get()
{
( import network
 network_isreachable_error ||return 1
  curl "http://download.finance.yahoo.com/d/quotes.csv?s=%40%5EDJI,$1&f=nsl1op"
)
}

quotes_search()
{
(
 import network
network_isreachable_error ||return 1
 import google
 local argv="$*"
 open http://finance.yahoo.com/lookup?s=$(url_encode "$argv")
)
 
}

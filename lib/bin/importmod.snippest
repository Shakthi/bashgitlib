importmod_imported=1
complete -W "$(import_list)"  importmod_edit
importmod_edit()
{
    local name=$1
    local fullname=$import_path/$name.snippest

    if [ -f $fullname ] ;then
        $BGIT_BASEDIR/bbin/backedit $fullname && source $fullname
    fi

}
